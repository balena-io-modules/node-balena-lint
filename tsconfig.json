{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2023",
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"preserveConstEnums": true,
		"removeComments": true,
		"sourceMap": true,
		"moduleResolution": "node",
		"baseUrl": ".",
		"skipLibCheck": true,
		"allowJs": true
	},
	"include": [
		"./lib/**/*",
		"config/eslint.config.js",
		"./test/**/*.ts",
		"eslint.config.js"
	]
}
